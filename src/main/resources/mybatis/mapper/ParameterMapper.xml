<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qjk.dao.ParameterDao">
    <select id="getParamByStateId" resultType="Parameter">
        select id, state_id, total_score, score_info from parameter where state_id = #{stateId}
    </select>
</mapper>